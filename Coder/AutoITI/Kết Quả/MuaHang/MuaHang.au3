


Func AutoMua20()
   Local $hWnd = WinWait("[CLASS:FSOnline Class]", "", 10); lay hwnd cua so
   ControlClick ( $hWnd, "", "", "left" , 1, 401, 329-23 );can phai sua de auto tim phan mua hang
   Sleep(500); cho 0.5s
   ControlClick ( $hWnd, "", "", "left" , 1, 311, 296-23 );chon dong de mua; mua 20 hang
   Sleep(500)
   ControlSend ( $hWnd, "","","20")
   Sleep(500)
   ControlClick ( $hWnd, "", "", "left" , 1, 307, 377-23 );xac nhan mua hang
   Sleep(500)
   ControlClick ( $hWnd, "", "", "left" , 1, 410, 382-23 );ket thuc mua hang
   Sleep(500)
EndFunc
